{
  "name": "openapi-ts-sdk-cli",
  "version": "1.0.1-alpha.0",
  "description": "A powerful CLI tool to generate TypeScript SDK clients from OpenAPI specifications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "bin": {
    "openapi-ts-sdk-cli": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "watch": "tsc --watch",
    "generate": "ts-node src/cli.ts generate",
    "clean": "rm -rf dist generated coverage && find src -name '*.js' -o -name '*.js.map' -o -name '*.d.ts' | xargs rm -f",
    "clean:build": "npm run clean && npm run build",
    "test": "npm run build && jest --config jest.config.js",
    "test:unit": "jest tests/unit --config jest.config.js",
    "test:integration": "jest tests/integration --config jest.config.js --passWithNoTests",
    "test:functional": "jest tests/unit/openapi-parser --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:all": "npm run test:unit && npm run test:integration",
    "coverage": "c8 npm run test:all",
    "prepublishOnly": "npm run clean:build && npm run test:all"
  },
  "keywords": [
    "openapi",
    "typescript",
    "sdk",
    "cli",
    "api-client",
    "code-generator",
    "swagger",
    "client-generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "change-case": "^5.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^9.4.1",
    "handlebars": "^4.7.7",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.9",
    "c8": "^8.0.0",
    "jest": "^29.5.0",
    "nyc": "^15.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
